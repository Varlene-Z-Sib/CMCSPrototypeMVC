<h1>Edit Lecturer</h1>
<form asp-action="EditLecturer" method="post">
    <input type="hidden" asp-for="Id" />
    <div>
        <label>Name</label>
        <input asp-for="Name" />
    </div>
    <div>
        <label>Email</label>
        <input asp-for="Email" />
    </div>
    <div>
        <label>Phone</label>
        <input asp-for="Phone" />
    </div>
    <div>
        <label>Address</label>
        <input asp-for="Address" />
    </div>
    <button type="submit">Save</button>
</form>
